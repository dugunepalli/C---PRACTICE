//ASCII VALUE TYPES
#include<stdio.h>
#include<ctype.h>   //Header File for Characters
int main()
{
    for(int i=0;i<128;i++)
    {
        char c=i;
        printf("Assci Value of %d is %c and type is:", i, i);
        if(isupper(c))
        {
            printf(" Upper \n");
        }
        else if(islower(c))
        {
            printf(" Lower\n");
        }
        else if(isdigit(c))
        {
            printf(" Number\n");
        }
        else if(isspace(c))
        {
            printf(" Space\n");
        }
        else if(ispunct(c))
        {
            printf(" Punctuations\n");
        }
        else
        {
            printf(" \t\n");
        }
    }
    return 0;
}

#include<stdio.h>
#include<ctype.h>
int main()
{
    char c;
    scanf("%c", &c);
    if(isupper(c))
        {
            printf(" Upper \n");
        }
        else if(islower(c))
        {
            printf(" Lower\n");
        }
        else if(isdigit(c))
        {
            printf(" Number\n");
        }
        else if(isspace(c))
        {
            printf(" Space\n");
        }
        else if(ispunct(c))
        {
            printf(" Punctuations\n");
        }
        else
        {
            printf(" Other\n");
        }
    return 0;
}
